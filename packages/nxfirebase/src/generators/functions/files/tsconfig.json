{
  "extends": "<%= offsetFromRoot %>tsconfig.base.json",    
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "<%= offsetFromRoot %>dist/out-tsc",
    "sourceMap": true,
    "strict": true,
    "target": "es2017"
  },
  "compileOnSave": true,
  "exclude": ["**/*.spec.ts"],
  "include": [
    "src",
    "<%= offsetFromRoot %>.eslintrc.js"
  ]
}
